<!DOCTYPE html>
<html lang="en" dir="ltr" data-startbar="light" data-bs-theme="light">

<head>
    <%- include('./title-meta') %>

    <!-- Optional CSS -->
    <%- typeof extra_css !== "undefined" ? extra_css : "" %>

    <%- include('./head-css') %>
</head>

<body id="body">

    <!-- Top Bar -->
    <%- include('./topbar') %>

    <!-- Left/Start Sidebar -->
    <%- include('./startbar') %>

    <div class="page-wrapper">
        <div class="page-content">

            <!-- Main Page Content -->
            <%- typeof content !== "undefined" ? content : "" %>

            <!-- Footer -->
            <%- include('./footer') %>

        </div>
    </div>

    <!-- Vendor JS -->
    <%- include('./vendorjs') %>

    <!-- Optional JS -->
    <%- typeof extra_javascript !== "undefined" ? extra_javascript : "" %>

    <script src="/js/app.js"></script>

</body>
</html>
